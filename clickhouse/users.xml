<?xml version="1.0"?>
<yandex>
    <users>
        <default>
            <password></password> <!-- пустой пароль для user default -->
            <networks>
                <ip>::/0</ip> <!-- разрешить доступ со всех IP -->
            </networks>
            <profile>default</profile>
            <quota>default</quota>
        </default>
    </users>

    <profiles>
        <default>
            <max_memory_usage>10000000000</max_memory_usage> <!-- лимит памяти -->
            <use_uncompressed_cache>0</use_uncompressed_cache>
        </default>
    </profiles>

    <quotas>
        <default></default>
    </quotas>
</yandex>
